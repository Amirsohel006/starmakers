<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenSevenVM" type="com.starmakers.app.modules.screenseven.data.viewmodel.ScreenSevenVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipsefortysix"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_3pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearSCREENSeven"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStackellipsefortyeight"
                android:layout_height="@dimen/_577pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackellipsefortysix"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_198pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_269pxh">
                    <ImageView android:id="@+id/imageEllipseFortySix"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_198pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse46_198x269}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse46_198x269"/>
                    <LinearLayout
                        android:id="@+id/linearColumnellipsefifty"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_60pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_60pxv"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_orange_a100_radius_3_87"
                            android:id="@+id/viewEllipseFifty"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_7pxh"/>
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtTaponScreen"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{screenSevenVM.screenSevenModel.txtTaponScreen}"
                            style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_tap_on_screen"/>
                    </LinearLayout>
                </FrameLayout>
                <View
                    android:background="@drawable/rectangle_bg_orange_a100_radius_17_67"
                    android:id="@+id/viewEllipseFortyEight"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_26pxv" android:layout_width="@dimen/_35pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_orange_a100_radius_7_18"
                    android:id="@+id/viewEllipseFortyNine"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_74pxv"
                    android:layout_marginEnd="@dimen/_76pxh"
                    android:layout_marginStart="@dimen/_76pxh"
                    android:layout_marginTop="@dimen/_74pxv" android:layout_width="@dimen/_14pxh"/>
                <LinearLayout
                    android:id="@+id/linearColumnintrestonloan"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <ImageView android:gravity="start"
                        android:id="@+id/imageIntrestonloan"
                        android:layout_height="@dimen/_250pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_120pxh}"
                        imageRes="@{@drawable/img_intrestonloan}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_intrestonloan"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_75pxh"
                        android:layout_marginStart="@dimen/_75pxh"
                        android:layout_marginTop="@dimen/_77pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{screenSevenVM.screenSevenModel.txtLanguage}"
                        style="@style/txtPoppinssemibold24_1" tools:text="@string/msg_direct_audition"/>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDoremipsumdol"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:text="@{screenSevenVM.screenSevenModel.txtDoremipsumdol}"
                        style="@style/txtPoppinsregular20" tools:text="@string/msg_dorem_ipsum_dol"/>
                </LinearLayout>
            </FrameLayout>

            <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                android:id="@+id/indicatorCar"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_130pxh"
                android:layout_marginStart="@dimen/_133pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="wrap_content"
                app:drawable_normal="@drawable/indicator_unselected_bg_gray_500_26_radius_8"
                app:drawable_selected="@drawable/indicator_selected_bg_gray_500_radius_8"
                app:indicator_size="@dimen/_13pxh" app:indicator_spacing="@dimen/_7pxh"/>
            <FrameLayout android:id="@+id/frameStackellipsefortyseven"
                android:layout_height="@dimen/_403pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_286pxh">
                <ImageView android:id="@+id/imageEllipseFortySeven"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_403pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse47_403x286}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse47_403x286"/>
                <LinearLayout android:id="@+id/linearRowskip"
                    android:layout_gravity="top|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSkip"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_weight="0.6"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{screenSevenVM.screenSevenModel.txtSkip}"
                        style="@style/txtPoppinsmedium24" tools:text="@string/lbl_skip"/>
                    <LinearLayout
                        android:id="@+id/linearRowellipsefiftyone"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_103pxh"
                        android:layout_weight="0.4"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_bg_orange_a100_radius_16_2"
                            android:id="@+id/viewEllipseFiftyOne"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginBottom="@dimen/_16pxv" android:layout_width="@dimen/_27pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_orange_a100_radius_6_58"
                            android:id="@+id/viewEllipseFiftyTwo"
                            android:layout_height="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_36pxv" android:layout_width="@dimen/_11pxh"/>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:background="@drawable/rectangle_bg_orange_a100_radius_3_545"
                    android:id="@+id/viewEllipseFiftyThree"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginBottom="@dimen/_75pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_75pxv" android:layout_width="@dimen/_6pxh"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="gridrectangletenRowModel" type="com.starmakers.app.modules.financialoverview.data.model.GridrectangletenRowModel"/>
    </data>
    <FrameLayout
        android:id="@+id/frameStackrectangleten"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">


        <androidx.cardview.widget.CardView
            android:id="@+id/cardview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true">
        <ImageView
            android:id="@+id/imageRectangleTen"
            android:layout_gravity="start"
            android:layout_height="@dimen/_264pxh"
            android:layout_width="@dimen/_159pxh"
            android:scaleType="fitXY"
            tools:ignore="ContentDescription" />
        <LinearLayout
            android:id="@+id/linearColumnmoviekantara"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical">
            <FrameLayout
                android:id="@+id/frameStackmoviekantara"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="wrap_content">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <TextView
                    android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtMovieKantara"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_movie_kantara"
                    style="@style/txtPoppinsregular12"
                    tools:text="@string/lbl_movie_kantara"/>
                    <TextView
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtMovieKantara1"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text=""
                        style="@style/txtPoppinsregular12"
                        tools:text=""/>

                </LinearLayout>

            </FrameLayout>
            <FrameLayout
                android:id="@+id/frameStackmoviekantara1"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <TextView
                    android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtGenreMystery"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_genre_mystery"
                    style="@style/txtPoppinsregular12"
                    tools:text="@string/lbl_genre_mystery"/>


                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtGenreMystery1"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text=""
                        style="@style/txtPoppinsregular12"
                        tools:text=""/>
                </LinearLayout>
            </FrameLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:id="@+id/linearRowviewdetails"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStylegreen_600cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtViewDetails"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content" android:singleLine="true"
                    android:text="@string/lbl_view_details"
                    style="@style/txtPoppinsregular12"
                    tools:text="@string/lbl_view_details"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700"/>
            </LinearLayout>
        </LinearLayout>
        </androidx.cardview.widget.CardView>
    </FrameLayout>
</layout>

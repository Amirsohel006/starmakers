<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gridrectangletenRowModel" type="com.starmakers.app.modules.financialoverview.data.model.GridrectangletenRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangleten"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageRectangleTen"
            android:layout_gravity="start"
            android:layout_height="@dimen/_264pxh"
            android:layout_width="@dimen/_159pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle10_264x159}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle10_264x159"/>
        <LinearLayout android:id="@+id/linearColumnmoviekantara"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackmoviekantara"
                android:layout_gravity="start"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh" android:layout_width="@dimen/_92pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtMovieKantara"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{gridrectangletenRowModel.txtMovieKantara}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_movie_kantara"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtGenreMystery"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{gridrectangletenRowModel.txtGenreMystery}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_genre_mystery"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowviewdetails"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegreen_600cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtViewDetails"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{gridrectangletenRowModel.txtViewDetails}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_view_details"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>

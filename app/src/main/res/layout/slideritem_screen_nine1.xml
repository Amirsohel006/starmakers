<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="imageSliderSliderintrestonloanModel" type="com.starmakers.app.modules.screennine.data.model.ImageSliderSliderintrestonloanModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnintrestonloan"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <ImageView android:gravity="start"
            android:id="@+id/imageIntrestonloan"
            android:layout_height="@dimen/_258pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageUrl="@{imageSliderSliderintrestonloanModel.imageIntrestonloan}"
            placeHolder="@{@drawable/image_not_found}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_intrestonloan_258x315"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtBookArtists"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_62pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{imageSliderSliderintrestonloanModel.txtBookArtists}"
            style="@style/txtPoppinssemibold24_1" tools:text="@string/lbl_book_artists"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDoremipsumdol"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:text="@{imageSliderSliderintrestonloanModel.txtDoremipsumdol}"
            style="@style/txtPoppinsregular20" tools:text="@string/msg_dorem_ipsum_dol"/>
    </LinearLayout>
</layout>

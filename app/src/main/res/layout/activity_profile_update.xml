                    android:id="@+id/forheight"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:background="@drawable/rectangle_border_gray_500_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_weight_in_kg"
                        android:id="@+id/etWeight" android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="Weight In KG"
                        android:textColorHint="@color/gray_802"
                        style="@style/etRoundedOutline_3"
                        tools:ignore="Autofill"/>

                    <EditText
                        android:background="@drawable/rectangle_border_gray_500_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_weight_in_kg"
                        android:id="@+id/etWeight1"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv" android:singleLine="true"
                        android:text="0Cm"
                        android:textColorHint="@color/gray_802"
                        style="@style/etRoundedOutline_3"
                        tools:ignore="Autofill"/>
                </LinearLayout>


                <EditText
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/rectangle_border_gray_500_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_location"
                    android:id="@+id/etLocation1"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv" android:singleLine="true"
                    android:textColorHint="@color/gray_802"
                    style="@style/etRoundedOutline_3"
                    tools:ignore="Autofill"/>


                <ProgressBar
                    android:visibility="gone"
                    android:id="@+id/progressBar"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>




            </LinearLayout>
        </FrameLayout>
    </ScrollView>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnUpdate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/gray_802"
        android:ellipsize="end"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_5pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_5pxv"
        android:singleLine="true"
        android:text="Update"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:text="Update" />
</LinearLayout>
                        android:textColorHint="@color/gray_802"
                        style="@style/etRoundedOutline_3"
                        tools:ignore="Autofill"/>

                    <EditText
                        android:background="@drawable/rectangle_border_gray_500_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_height_in_cm"
                        android:id="@+id/etHeight1"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv" android:singleLine="true"
                        android:text="0Cm"
                        android:textColorHint="@color/gray_802"
                        style="@style/etRoundedOutline_3"
                        tools:ignore="Autofill"/>
                </LinearLayout>



                </FrameLayout>

                <EditText
                    android:id="@+id/etName"
                    style="@style/etRoundedOutline_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:background="@drawable/rectangle_border_bluegray_101_radius_10"
                    android:ellipsize="end"
                    android:inputType="text"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:singleLine="true"
                    tools:ignore="Autofill" />

                <LinearLayout
                    android:id="@+id/linearColumnmobileno"
                    style="@style/groupStylecornerRadius_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/etMobileNo"
                        style="@style/etRoundedOutline_3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/rectangle_border_bluegray_101_radius_10"
                        android:ellipsize="end"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="8593838738"
                        android:textColor="@color/gray_600_3f1"
                        tools:ignore="Autofill" />
                </LinearLayout>

                <TextView
                    android:id="@+id/etEmail"
                    style="@style/etRoundedOutline_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:background="@drawable/rectangle_border_bluegray_101_radius_10"
                    android:ellipsize="end"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="Email"
                    android:textColor="@color/gray_600_3f1"
                    tools:ignore="Autofill" />

                <TextView
                    android:id="@+id/etLocation"
                    style="@style/etRoundedOutline"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:background="@drawable/rectangle_border_bluegray_101_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_location"
                    android:inputType="text"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text=""
                    android:textColorHint="@color/black_900"
                    android:visibility="gone"
                    tools:ignore="Autofill" />





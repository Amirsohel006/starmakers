        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:clipToPadding="false"
        android:fillViewport="true">

        <FrameLayout
            android:id="@+id/frameStackellipseseven"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_858pxv"
            android:layout_marginTop="@dimen/_5pxv">

            <LinearLayout
                android:id="@+id/linearColumnellipseseven"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/linearRowellipseseven"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageEllipseSeven"
                        android:layout_width="@dimen/_129pxh"
                        android:layout_height="@dimen/_129pxh"
                        android:layout_marginStart="80dp"
                        android:layout_marginBottom="@dimen/_30pxv"
                        android:scaleType="centerCrop"
                        android:src="@drawable/img_ellipse7"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_ellipse7" />

                    <ImageView
                        android:id="@+id/editProfileImageView"
                        android:layout_width="34dp"
                        android:layout_height="34dp"
                        android:layout_gravity="bottom|center_horizontal"
                        android:layout_marginBottom="30dip"
                        android:src="@drawable/edit" />

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="campaignOneVM" type="com.starmakers.app.modules.campaignone.data.viewmodel.CampaignOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_802">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_52pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKANTARA"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_180pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{campaignOneVM.campaignOneModel.txtKANTARA}"
                    style="@style/txtPoppinssemibold24" tools:text="@string/lbl_kantara3"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackrectangleten"
                android:layout_height="@dimen/_918pxv" android:layout_width="match_parent">
                <HorizontalScrollView android:fillViewport="true"
                    android:id="@+id/horizontalScrollHorizontalscrollrectangleten"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_width="match_parent" android:scrollbars="none">
                    <LinearLayout
                        android:id="@+id/linearColumnrectangleten"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <ImageView android:id="@+id/imageRectangleTen"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_424pxh"
                            android:layout_width="@dimen/_355pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_rectangle10_424x355}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle10_424x355"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMovieKantara"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtMovieKantara}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/lbl_movie_kantara"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreMystery"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtGenreMystery}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/lbl_genre_mystery"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtStoryline"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtStoryline}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/lbl_storyline"/>
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtDescription"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:text="@{campaignOneVM.campaignOneModel.txtDescription}"
                            style="@style/txtPoppinsregular14_7" tools:text="@string/msg_in_1847_there"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAppxBudget"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtAppxBudget}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/lbl_appx_budget"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMovieStartDat"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtMovieStartDat}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/msg_movie_start_dat"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMovieEndDate"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtMovieEndDate}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/msg_movie_end_date"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtReleasingDate"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtReleasingDate}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/msg_releasing_date"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTimeZone"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{campaignOneVM.campaignOneModel.txtTimeZone}"
                            style="@style/txtPoppinsregular16_4" tools:text="@string/lbl_cast"/>
                        <LinearLayout android:gravity="end"
                            android:id="@+id/linearRowrectangle147"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageRectangle147"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle147}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle147"/>
                            <ImageView
                                android:id="@+id/imageRectangle148"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle148}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle148"/>
                            <ImageView
                                android:id="@+id/imageRectangle149"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle149}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle149"/>
                            <ImageView
                                android:id="@+id/imageRectangle150"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle150}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle150"/>
                            <ImageView
                                android:id="@+id/imageRectangle151"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle151}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle151"/>
                            <ImageView
                                android:id="@+id/imageRectangle152"
                                android:layout_height="@dimen/_78pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_78pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle152}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle152"/>
                        </LinearLayout>
                    </LinearLayout>
                </HorizontalScrollView>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnParticipate"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_220pxv"
                    android:layout_marginTop="@dimen/_220pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_participate"
                    android:textAllCaps="false" style="@style/btnSolid" tools:text="@string/lbl_participate"/>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrectangle107RowModel" type="com.starmakers.app.modules.selectionlistone.data.model.Listrectangle107RowModel"/>
    </data>
    <LinearLayout
        android:background="@drawable/rectangleforaudition"
        android:id="@+id/linearRowrectangle106"
        android:layout_height="@dimen/_240pxh"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        style="@style/groupStylewhite_9">
        <ImageView
            android:id="@+id/imageRectangle106"
            android:layout_height="@dimen/_200pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_117pxh"
            android:scaleType="fitXY"
            cornerRadius="@{@dimen/_5pxh}"
            imageRes="@{@drawable/img_rectangle106_178x117}"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_rectangle106_178x117"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumndate"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="0.61" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowdate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDate"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_date"
                    style="@style/txtPoppinsmedium14"
                    tools:text="@string/lbl_date"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDateOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="100dp"
                    android:singleLine="true"
                    android:text="@string/lbl_26_05_2023"
                    style="@style/txtPoppinsmedium14_2"
                    tools:text="@string/lbl_26_05_2023"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtime"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_time"
                    style="@style/txtPoppinsmedium14"
                    tools:text="@string/lbl_time"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_10am_12am"
                    style="@style/txtPoppinsmedium14_2"
                    tools:text="@string/lbl_10am_12am"/>

                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_10am_12am"
                    style="@style/txtPoppinsmedium14_2"
                    tools:text="@string/lbl_10am_12am"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtVenue"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_venue"
                style="@style/txtPoppinsregular14_1"
                tools:text="@string/lbl_venue"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_width="match_parent"
                android:text="@string/msg_gate_no_3_ba"
                style="@style/txtPoppinsregular12_1"
                tools:text="@string/msg_gate_no_3_ba"/>

            <LinearLayout
                android:id="@+id/movie_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/moviename"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Movie:"
                    style="@style/txtPoppinsmedium14"/>
                <TextView
                    android:id="@+id/movie"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Movie:"
                    android:layout_marginStart="@dimen/_5pxh"
                    style="@style/txtPoppinsmedium14_2"/>



            </LinearLayout>


            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrole"
                android:layout_height="wrap_content"
                android:layout_marginEnd="2dp"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRole"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_role"
                    style="@style/txtPoppinsmedium14"
                    tools:text="@string/lbl_role"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDancer"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_dancer"
                    style="@style/txtPoppinsmedium14_2"
                    tools:text="@string/lbl_dancer"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSelectionListOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@string/lbl_selection_list"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_3" tools:text="@string/lbl_selection_list"/>
        </LinearLayout>
    </LinearLayout>
</layout>

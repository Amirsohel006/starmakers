<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".modules.help.ui.HelpDetailActivity">
    <androidx.appcompat.widget.Toolbar android:gravity="start"
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" app:contentInsetStart="0dp">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_802">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtHelpFAQs"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_weight="0.86"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_help_faqs"
                style="@style/txtPoppinssemibold24"
                tools:text="@string/lbl_help_faqs"/>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:layout_marginTop="5dp"
        android:id="@+id/title1"
        android:text="question"
        style="@style/txtPoppinsregular14"
        android:layout_marginStart="@dimen/_24pxh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <TextView
        android:layout_marginStart="@dimen/_24pxh"
        android:layout_marginEnd="@dimen/_24pxh"
        android:id="@+id/description1"
        android:text="Hello   Everyone How you guys are doing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</LinearLayout>
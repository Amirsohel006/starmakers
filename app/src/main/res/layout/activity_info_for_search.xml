<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:gravity="start"
    android:id="@+id/linearAuditionsOne"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/groupStylewhite_A700"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" app:contentInsetStart="0dp">
        <LinearLayout
            android:gravity="start"
            android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            style="@style/groupStylegray_802">
            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFinanceOvervie"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginEnd="@dimen/_67pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_weight="0.9"
                android:layout_width="0dp" android:singleLine="true"
                android:text="@{auditionsOneVM.auditionsOneModel.txtFinanceOvervie}"
                style="@style/txtPoppinssemibold24" tools:text="@string/msg_finance_overvie"/>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>


    <LinearLayout android:id="@+id/linearRowrectangleten"
        android:layout_gravity="top|center"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_10pxv"
        android:layout_marginEnd="@dimen/_12pxh"
        android:layout_marginStart="@dimen/_12pxh"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        style="@style/groupStylegray_52cornerRadius">
        <ImageView android:id="@+id/imageRectangleTen"
            android:layout_height="@dimen/_147pxh"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="@dimen/_117pxh"
            android:scaleType="fitXY"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_rectangle10_264x159"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnmoviekantara"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtMovieKantara"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtMovieKantara}"
                    style="@style/txtPoppinsmedium14_3"
                    tools:text="@string/lbl_movie_kantara"/>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtGenreMystery"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtGenreMystery}"
                    style="@style/txtPoppinsmedium14_3"
                    tools:text="@string/lbl_genre_mystery"/>


            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtStoryline"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtStoryline}"
                    style="@style/txtPoppinsmedium14_3"
                    tools:text="@string/lbl_storyline"/>


            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAppxBudget"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtAppxBudget}"
                    style="@style/txtPoppinsmedium14_3"
                    tools:text="@string/lbl_appx_budget"/>

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtReleasingDate"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtReleasingDate}"
                    style="@style/txtPoppinsmedium14_3"
                    tools:text="@string/msg_releasing_date"/>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>




    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:id="@+id/frameStackspentamount"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">


            <LinearLayout
                android:background="@drawable/backgroundbudget"
                android:id="@+id/linearColumnspentamount"
                android:layout_gravity="start"
                android:layout_height="600dp"
                android:layout_width="match_parent"
                android:orientation="vertical"
                style="@style/groupStylewhite_11">
                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtSpentAmount"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="20dp"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{auditionsOneVM.auditionsOneModel.txtSpentAmount}"
                    style="@style/txtPoppinsmedium16_2"
                    tools:text="@string/lbl_spent_amount"/>


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{auditionsOneVM.auditionsOneModel.txtPrice}"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="@string/msg_collected_budge"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice1"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="1000000"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="1000000"/>
                </LinearLayout>



                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPriceOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{auditionsOneVM.auditionsOneModel.txtPriceOne}"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="@string/msg_spent_amount"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPriceOne1"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{auditionsOneVM.auditionsOneModel.txtPriceOne}"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="@string/msg_spent_amount"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPriceTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{auditionsOneVM.auditionsOneModel.txtPriceTwo}"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="@string/msg_required_addnl"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPriceTwo1"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{auditionsOneVM.auditionsOneModel.txtPriceTwo}"
                        style="@style/txtPoppinsregular14_8"
                        tools:text="@string/msg_required_addnl"/>

                </LinearLayout>



            </LinearLayout>

        </FrameLayout>
    </ScrollView>
</LinearLayout>